{
    "name": "xanpena/svg-chart-builder",
    "description": "PHP SVG Chart Builder",
    "keywords": [
        "php",
        "svg",
        "charts",
        "pdf"
    ],
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4" : {
            "Xanpena\\SVGChartBuilder\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4" : {
            "Xanpena\\SVGChartBuilder\\Tests\\" : "tests"
        }
    },
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    }
}
